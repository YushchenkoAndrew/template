{
  "name": "webserver",
  "version": "1.0.5",
  "description": "webserver for some simple projects",
  "author": "Andrew Y (Mortis)",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "docker:build": "docker build -t grimreapermortis/webserver:1.0.5 .",
    "docker:push": "docker push grimreapermortis/webserver:1.0.5",
    "kube:add": "kubectl apply -f kubernetes/",
    "kube:del": "kubectl delete -f kubernetes/",
    "kube:get": "kubectl get pods -n template",
    "kube:scale": "kubectl scale deployment web-deployment --replicas=4 -n template"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "get-image-colors": "^4.0.0",
    "get-pixels": "^3.3.2",
    "jpeg-js": ">=0.4.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "pg": "^8.3.3",
    "sequelize": "^6.3.4"
  }
}
